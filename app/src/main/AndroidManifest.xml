<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="io.github.yehiareda.shapecontainer.sample">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="io.github.yehiareda.shapecontainer.sample.ShapeOfViewMainActivity"
            android:exported="true" />

        <activity
            android:name="io.github.yehiareda.shapecontainer.sample.ShapeOfViewAnimationActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="io.github.yehiareda.shapecontainer.sample.ShapeOfViewStarActivity"
            android:exported="true" />
        <activity
            android:name="io.github.yehiareda.shapecontainer.sample.MaterialDesignShrineActivity"
            android:exported="true" />
        <activity
            android:name="io.github.yehiareda.shapecontainer.sample.ErrorActivity"
            android:exported="true" />
        <activity
            android:name="io.github.yehiareda.shapecontainer.sample.JackmanActivity"
            android:exported="true" />
        <activity
            android:name="io.github.yehiareda.shapecontainer.sample.StarWarsActivity"
            android:exported="true" />
        <activity
            android:name="io.github.yehiareda.shapecontainer.sample.ShapeOfViewRoundActivity"
            android:exported="true" />
        <activity
            android:name="io.github.yehiareda.shapecontainer.sample.DottedEdgesActivity"
            android:exported="true" />
        <activity
            android:name="io.github.yehiareda.shapecontainer.sample.ShapeOfViewStarAnimatedActivity"
            android:exported="true" />

    </application>

</manifest>